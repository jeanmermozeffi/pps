{% extends 'AvanzuAdminThemeBundle:layout:base-layout.html.twig' %}
{% block page_content %}
    <div class="row">
        <div class="col-md-10">
             <ul class="list-unstyled" style="margin-bottom: 15px;">
                                <li><h4>Fichiers Joints</h4></li>
                            {% for fichier in consultation.fichiers %}
                                <li>
                                    <a href="{{ path('admin_parametre_fichier_show', {'id': fichier.id}) }}">
                                        <span class="fa fa-file"></span> {{ fichier.title }}
                                    </a>
                                </li>
                            {% endfor %}
                            </ul>
        </div>
        <div class="col-md-12">
            <div class="box">
                <div class="box-body">
                   
                    <div class="row">
                        <div class="col-md-10 col-md-offset-1">
                            
                            <iframe src="{{ path('admin_consultation_print',{'id': id}) }}"  width="100%" height="700px"></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
{% block page_title %}
    Impression de la consultation

{% endblock %}