{% extends 'AvanzuAdminThemeBundle:layout:base-layout.html.twig' %}

{% import 'GestionBundle:Macros:html.html.twig' as _html %}



{% block page_content %}

    <div class="row">

        <!-- /.col -->

        <div class="col-md-12">

            

            {{ form_errors(form)}}

            {% if errors|length %}

                <div class="alert alert-danger">

                    {% for error in errors %}

                        <p>{{ error }}</p>

                    {% endfor %}

                </div>

            {% endif %}

            {{ form_start(form, { 'attr': {'class': 'form-horizontal', 'data-parsley-validate': true} }) }}

            <div class="form-group">

                <div class="col-sm-12">

                    <label class="col-sm-4 control-label">{{ form_label(form.dateCreation) }}</label>

                    <div class="col-sm-8">

                        {{ form_widget(form.dateCreation) }}

                    </div>

                </div>



            </div>

            <div class="form-group">

                <div class="col-sm-8 col-sm-offset-4">

                    <input type="submit" class="btn btn-success" value="Valider"/>

                </div>

            </div>

            {{ form_end(form) }}

            <!-- /.nav-tabs-custom -->

        </div>

        <!-- /.col -->

    </div>



{% endblock %}





{% block page_title %}

   {{ _html.text('pass.export') }}

{% endblock %}



{% block page_subtitle %}

{% endblock %}