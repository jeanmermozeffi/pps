{% if objet.data %}
<div class="box box-info">
    <div class="box-header with-border">
        <h3 class="box-title">{{ message }}</h3>

        <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
            </button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
        </div>
    </div>
    <div class="box-body">
        {% if total is defined and consultation is defined %}
            {{ consultation }} / {{ total }} patient(s) ayant au moins consult√© une fois avec le PASS
        {#<canvas id="medicament" style="height:250px"></canvas>#}
        {% endif %}

        {% if total is defined and (corporate is defined and corporate) %}
            <p>{{ total.s | default(0) }} PASS</p>
        {% endif %}
        <div id="{{ objet.container }}" class="chart">
        </div>
        
    </div>
    <!-- /.box-body -->
</div>
{% else %}
<div id="{{ objet.container }}" class="chart hide"></div>
{% endif %}

{% block javascripts_inline %}
