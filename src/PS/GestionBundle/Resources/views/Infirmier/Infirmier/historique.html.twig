{% extends 'AvanzuAdminThemeBundle:layout:base-layout.html.twig' %}
{% block page_title %}
    Mon historique
{% endblock %}
{% block page_content %}
    <div class="row">
        <div class="col-md-12">
            <!-- general form elements -->
            <div class="box box-primary">
                <div class="box-header">
                    <!-- Horizontal Form -->
                    <div class="">
                        <div class="box-header with-border">
                            <h3 class="box-title">{{ title|default('Liste') }}</h3>
                        </div>
                    </div>
                </div>
                <div class="box-body">
                    {{ grid(grid) }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}